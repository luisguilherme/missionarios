<h1>New telefone</h1>

<% form_for(@telefone) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :missionario %><br />
    <%= f.text_field :missionario %>
  </p>
  <p>
    <%= f.label :tipo_telefone %><br />
    <%= f.text_field :tipo_telefone %>
  </p>
  <p>
    <%= f.submit "Create" %>
  </p>
<% end %>

<%= link_to 'Back', telefones_path %>
